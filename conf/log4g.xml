<!-- seelog configuration file -->
<seelog minlevel="debug" maxlevel="error">
    <outputs formatid="main">
        <console />
        <rollingfile type="date" filename="logs/app.log" namemode="prefix" maxrolls="5" datepattern="2006.01.02" />
    </outputs>
    <formats>
        <format id="main" format="[%Date %Time][%Level] %Func: %Msg%n" />
    </formats>
</seelog>